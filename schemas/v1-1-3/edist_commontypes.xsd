		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MetadataObjectBaseType">
				<xsd:sequence>
					<xsd:element ref="Contacts" minOccurs="0"/>
					<xsd:element name="Directions" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Direction(s).</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element ref="Direction" maxOccurs="unbounded"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
