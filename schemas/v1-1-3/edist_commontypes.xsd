						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Agent sign on (SINE). Example: 6146TL</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Terminal" type="ContextSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Agent CRT or terminal LNIATA number. Example: 956301007</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Department" type="ProperNameSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Department name or identifier.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DepartmentCode" type="CodesetValueSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Carrier or aggregator department code.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="AirlineSpecificCodes" type="CodesetValueSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Services record owner.
Example: ATO (ALL AIRLINE TICKET OFFICES)</xsd:documentation>
							<xsd:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
=enumerated values=
RES = ALL RESERVATION CENTERS
ATO = ALL AIRLINE TICKET OFFICES
CTO = ALL CITY TICKET OFFICES
WEB = WEBSITE</xsd:documentation>
							<xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. These are airline-specific codes applicable for the Services record owner or the carrier specified in bytes 23-25.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
