	<xsd:complexType name="RuleMetadataType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for RULE Metadata.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MetadataObjectBaseType">
				<xsd:sequence>
					<xsd:element name="RuleID">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Unique Rule identifier (assigned by owner.) Example: 1</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:extension base="xsd:string"/>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="Name" type="ProperNameSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Rule name.  Example: Rule001</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Status" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Rule status.  Examples: ActionRequired, RuleExecuted, Other</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:extension base="RuleStatusSimpleType"/>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="Values" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Rule Value(s).</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Value" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Rule value detail.</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element ref="NodePath" minOccurs="0" maxOccurs="unbounded"/>
											<xsd:element name="Instruction">
												<xsd:annotation>
													<xsd:documentation source="description" xml:lang="en">Business rule instruction.
Example: [NodePath.r1] {EQ('FR')} [NodePath.r2]{EQ(TRUE)}</xsd:documentation>
												</xsd:annotation>
												<xsd:complexType>
													<xsd:simpleContent>
														<xsd:extension base="xsd:string"/>
													</xsd:simpleContent>
												</xsd:complexType>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element ref="Remarks" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
