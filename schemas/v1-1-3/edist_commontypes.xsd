	<!--     << Offer/ Offer Item Pricing >>-->
	<xsd:simpleType name="OfferPriceAssocListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Offer Price Association Method list constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:whiteSpace value="preserve"/>
			<xsd:enumeration value="MultiSegOutboundOD_Pair">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">One Fare Price is Specified for the Outbound Leg (regardless of number of flight segments) with Associations to Related Flight Segment O/D Pairs</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PerFlightOD_Pair">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">One Fare Price is Specified Per Flight Segment O/D Pair</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OfferPriceAssocSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Offer Price Association.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType OfferPriceAssocListType"/>
	</xsd:simpleType>
	<xsd:simpleType name="OfferPricingSysListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Offer Pricing Method constrained list.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:whiteSpace value="preserve"/>
			<xsd:enumeration value="AirlineReservationSystem"/>
			<xsd:enumeration value="AutoExchange"/>
			<xsd:enumeration value="CentralReservationSystem"/>
			<xsd:enumeration value="OfferEngine"/>
			<xsd:enumeration value="PricingEngine"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OfferPricingSystemSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType OfferPricingSysListType"/>
	</xsd:simpleType>
