	<xsd:simpleType name="SeatAssocSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Seat Association Base List.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AircraftEquipment"/>
			<xsd:enumeration value="ClassOfService"/>
			<xsd:enumeration value="FlightGroup"/>
			<xsd:enumeration value="FlightOriginDestination"/>
			<xsd:enumeration value="FlightSegment"/>
			<xsd:enumeration value="MediaGroup"/>
			<xsd:enumeration value="MediaItem"/>
			<xsd:enumeration value="Offer"/>
			<xsd:enumeration value="OfferItem"/>
			<xsd:enumeration value="Penalty"/>
			<xsd:enumeration value="Service"/>
			<xsd:enumeration value="ServiceBundle"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="SeatAssocType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">General Seat Associations.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Association" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">General Seat Association Detail.

Note: Includes an association type choice (e.g. MediaGroup) and an instance association value (e.g. mediagrp1).</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Type" type="SeatAssocListType">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Named association type. Examples: AircraftEquipment, MediaGroup</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ReferenceValue" type="xsd:IDREF">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Associated object instance value. Example: mediagrp1</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
