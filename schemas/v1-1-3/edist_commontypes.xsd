	<xsd:simpleType name="BrandFareAssocListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type Branded Fare Association Type Name.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType BrandFareOfferAssocSimpleType"/>
	</xsd:simpleType>
	<xsd:simpleType name="OfferItemAssocSimpleType">
