<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2014 rel. 2 sp1 (x64) (http://www.altova.com) by Rastko Ilic (JR Technologies, Inc.) -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2014.2" id="NDC2014.2">
	<xsd:annotation>
		<xsd:documentation xml:lang="en">NDC XML SPECIFICATION: Common XML Objects shared by all NDC Domains.</xsd:documentation>
	</xsd:annotation>
	<xsd:annotation>
		<xsd:documentation xml:lang="en">Notice: This is NDC Draft Specification provided for pre-review by the members of the relevant governance groups, it is not provided for implementation in a production environment or considered to be the final  schema to support NDC business requirements. The draft schema may, at any time up to final specification submission, undergo changes including, but not limited to, specific elements, element names/labels, annotations or message name as a result of the ongoing quality control process, and comments, which may be received following the pre-review. Implementation of this version is done so at the risk of the implementer or implementing company.</xsd:documentation>
	</xsd:annotation>
	<xsd:include schemaLocation="edist_structures.xsd"/>
	<!--
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    A D D R E S S
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
	<xsd:element name="Address">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Address representation choice.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="AddressCore"/>
				<xsd:element ref="AddressDetail"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AddressCore" type="AddressCoreType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Core Address Information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AddressDetail" type="AddressDetailType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Detailed Address Information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="AddressCoreType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">CORE ADDRESS representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="KeyWithMetaObjectBaseType">
				<xsd:sequence>
					<xsd:element name="Address">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Address.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element ref="PaymentAddress"/>
								<xsd:element ref="SimpleAddress"/>
								<xsd:element ref="StructuredAddress"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AddressDetailType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">DETAIL ADDRESS representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AddressCoreType">
				<xsd:sequence>
					<xsd:element name="Directions" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Address Directions.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element ref="Direction" maxOccurs="unbounded"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="SimpleAddress" type="SimpleAddrType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">SIMPLE (non-structured) ADDRESS.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="StructuredAddress" type="StructuredAddrType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">STRUCTURED ADDRESS.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PaymentAddress" type="PaymentAddrType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">PAYMENT ADDRESS.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="PaymentAddrType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">PAYMENT ADDRESS representation.

Note: This a "relaxed cardinality" representation of Structured Address to support various combinations of Address Verification fields.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AssociatedObjectBaseType">
				<xsd:sequence>
					<xsd:element name="Street" type="xsd:string" minOccurs="0" maxOccurs="2">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Street. Examples: Main Street, 100 Main Street</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PO_Box" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Post office box. Example: P.O. Box 21</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="BuildingRoom" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Building name, room, apartment, or suite number.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="City" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">City name. Example: Tampa</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:extension base="ProperNameSimpleType"/>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="StateProv" type="ProperNameSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">State or province code or name. Examples: NY, New York</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PostalCode" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Postal code. Examples: 11001, 11001-2345</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Country" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Country name or code.
Examples: US, United States

Encoding Scheme: ISO 3166-1 (two character) Country Code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SimpleAddrType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">SIMPLE ADDRESS representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AssociatedObjectBaseType">
				<xsd:sequence>
					<xsd:element name="AddressLine" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Address line text.
Example:
100 Main Street
New York, NY 10100</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="1"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="StructuredAddrType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">STRUCTURED ADDRESS representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AssociatedObjectBaseType">
				<xsd:sequence>
					<xsd:element name="Street" type="xsd:string" minOccurs="0" maxOccurs="2">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Street. Examples: Main Street, 100 Main Street</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="BuildingRoom" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Building name, room, apartment, or suite number.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PO_Box" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Post office box. Example: P.O. Box 21</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="CityName" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element ref="CityCode" minOccurs="0"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="StateProv" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">State or province code. Examples: NY, QUE</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PostalCode" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Postal code. Examples: 11001, 11001-2345</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="15"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element ref="CountryCode" minOccurs="0"/>
					<xsd:element name="County" type="ProperNameSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">County that the address is located in. Example: Litchfield</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!--
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    AI R   P A S S  P R E P A I D  P R O G R A M
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
	<xsd:element name="PrePaidProgramGroup" abstract="true">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Pre-paid Program(s) qualifiers.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AirPassCore" type="AirPassCoreType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Core Air Pass information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AirPassDetail" type="AirPassDetailType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Detail Air Pass information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!--<< Basic Definition >>-->
	<xsd:complexType name="AirPassCoreType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">AIR PASS CORE representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="KeyWithMetaObjectBaseType">
				<xsd:sequence>
					<xsd:element ref="AirlineID">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Air Pass Program issuing airline designator code. Example: BA

Encoding Scheme: IATA/ A4A (two to three character) Airline Designator Code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Certificate" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Airr Pass Certificate information.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:complexContent>
								<xsd:extension base="AssociatedObjectBaseType">
									<xsd:sequence>
										<xsd:element name="Number">
											<xsd:annotation>
												<xsd:documentation source="description" xml:lang="en">Unique certificate number. Example: 12345678</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:simpleContent>
													<xsd:extension base="ProperNameSimpleType">
														<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
													</xsd:extension>
												</xsd:simpleContent>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="Application" type="ContactAppSimpleType" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation source="description" xml:lang="en">Certificate Application.  Example: Redemption</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="EffectivePeriod" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation source="description" xml:lang="en">Certificate Effective/ Expiration Period.</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element ref="YearPeriod"/>
													<xsd:element ref="YearMonthPeriod"/>
													<xsd:element ref="TimePeriod"/>
													<xsd:element ref="QuarterPeriod"/>
													<xsd:element ref="MonthPeriod"/>
													<xsd:element ref="DayPeriod"/>
													<xsd:element ref="DateTimePeriod"/>
													<xsd:element ref="DatePeriod"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:extension>
							</xsd:complexContent>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AirPassDetailType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">AIR PASS DETAIL representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AirPassCoreType">
				<xsd:sequence>
					<xsd:element name="ProgramName">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Program Name. Example: PASSPRO</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:extension base="ProperNameSimpleType"/>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="ProgramCode" type="CodesetValueSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Program Code. Example: PP01</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Holder" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Agency or Corporate Certificate holder or program owner information.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:choice>
								<xsd:element ref="AgencyID"/>
								<xsd:element ref="PartnerID"/>
							</xsd:choice>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!--
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    A I R P O R T
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
	<!--<< Airport Code >>-->
	<xsd:element name="AirportCode">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Airport or City Code.  Example: TPA

Encoding Scheme: IATA/ A4A Aiport or City Code</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="AirportCitySimpleType">
					<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="AirportCodeType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Airport Code Identifier.

Encoding Scheme: IATA/ A4A Aiport or City Code</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="AirportCitySimpleType">
				<xsd:attributeGroup ref="ObjectPolicyMetaAttrGroup"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--<< Airport Definition >>-->
	<xsd:element name="AirportGroup" type="AirportCoreType" abstract="true">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Airport representation choice.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AirportCore" type="AirportCoreType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">CORE Airport Information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AirportSummary" type="AirportSummaryType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">SUMMARY Airport Information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AirportDetail" type="AirportDetailType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">DETAIL Airport Information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!--<< Basic Definition >>-->
	<xsd:complexType name="AirportCoreType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">CORE AIRPORT representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="KeyWithMetaObjectBaseType">
				<xsd:sequence>
					<xsd:element ref="AirportCode"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AirportSummaryType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">SUMMARY AIRPORT representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AirportCoreType">
				<xsd:sequence>
					<xsd:element name="Name" type="ProperNameSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Airport Name. Example: Tampa International Airport</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AirportDetailType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">DETAIL AIRPORT representation.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AirportSummaryType">
				<xsd:sequence>
					<xsd:element name="Terminal" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Airport terminal and gate.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:attribute name="Name" type="ProperNameSimpleType" use="optional">
								<xsd:annotation>
									<xsd:documentation source="description" xml:lang="en">Terminal name or ID.  Example: Terminal C</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
							<xsd:attribute name="Gate" type="ContextSimpleType" use="optional">
								<xsd:annotation>
									<xsd:documentation source="description" xml:lang="en">Terminal Gate Number.  Example: C39</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!--
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    A T P C O  R E C O R D  A T T R I B U T E S  /  D I A G N O S T I C S
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
	<xsd:element name="Attributes" type="AttributesType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">S5 Record Attributes information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="S5_Diagnostics" type="S5Type">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Record S5 Diagnostic Information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="S7_Diagnostics" type="S7Type">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Record S7 Diagnostic Information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Diagnostics" type="DiagnosticsType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Diagnostic information.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!--<< Basic Definitions >>-->
	<xsd:complexType name="AttributesType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">S5 record attributes definition.</xsd:documentation>
			<xsd:documentation source="ENCODING" xml:lang="en">ENCODING: Note, Attributes on the Services Record S5 that can be used as filter criteria for a service.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AssociatedObjectBaseType">
				<xsd:sequence>
					<xsd:element name="Group">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Service group information.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Code" type="CodesetValueSimpleType">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Service Group code.

BD (BUNDLED SERVICE)
BG (BAGGAGE)
IE (IN-FLIGHT ENTERTAINMENT)
LG (LOUNGE)
</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="Text" type="DescriptionSimpleType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Service group description.
Example: BAGGAGE OPTIONS</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="SubGroup" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Service subgroup information.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Code" type="CodesetValueSimpleType">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Service Sub-Group Code(s).

Group BG: CT (Container), PP (Pre-Paid Excess Baggage)
Group IE: IT (Internet Access), TV (Television)
</xsd:documentation>
										<xsd:documentation source="ENCODING" xml:lang="en">ENCODING: Note, Standard Group and Sub Group on the Services Record S5 that can be used as filter criteria for a service.
</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="Text" type="DescriptionSimpleType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Service subgroup description.
Example: CARRY ON HAND BAGGAGE</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="Desc1" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Description 1 codes.</xsd:documentation>
							<xsd:documentation source="ENCODING" xml:lang="en">ENCODING: Note, standard Description codes (or text translation for these codes) on the Optional Services Record S5.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Code" type="CodesetValueSimpleType">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Description code.
Example: 05 (UP TO 5 KG)</xsd:documentation>
										<xsd:documentation source="ENCODING" xml:lang="en">=sample values=
05 = up to 5 kg
1Y = One Year
AI = Aisle
B1 = First Bag</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="Text" type="DescriptionSimpleType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Description code text.
Example: UP TO 5 KG</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="Desc2" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Description 2 codes.</xsd:documentation>
							<xsd:documentation source="ENCODING" xml:lang="en">ENCODING: Note, standard Description codes (or text translation for these codes) on the Optional Services Record S5.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Code" type="CodesetValueSimpleType">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Description code.
Example: HT (HOT)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="Text" type="DescriptionSimpleType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Description code text.
Example: HOT</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="S5Type">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">record S5 diagnostic definition.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AssociatedObjectBaseType">
				<xsd:sequence>
					<xsd:element name="UID" type="UniqueStringID_SimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Unique ID. Example 6</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="S7Type">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Record S7 diagnostic definition.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AssociatedObjectBaseType">
				<xsd:sequence>
					<xsd:element name="UID" type="UniqueStringID_SimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Unique ID. Example: 6</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Seq" type="xsd:integer" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Table sequence number. Example: 3</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ExcessFirst" type="xsd:integer" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">ID of the first occurrence of an excess bag. Example: 4</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ExcessLast" type="xsd:integer" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">ID of the last occurrence of an excess bag. Example: 6</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DiagnosticsType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Diagnostic definition.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ID" type="UniqueStringID_SimpleType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Unique ID. Example 6</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AugmentationPoint" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!--
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    B A G G A G E
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
	<!--<< Basic Definitions >>-->
	<xsd:complexType name="BagTicketBoxType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Baggage Ticket Box.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AssociatedObjectBaseType">
				<xsd:sequence>
					<xsd:element name="TicketBoxText" type="ContextSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Ticket Box Text.  Example: 1PC (One Piece)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Concept" type="ContextSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Ticket box context.  Example: PIECE</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="BagAllowanceBaseType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Baggage Allowance.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="KeyWithMetaObjectBaseType">
				<xsd:sequence>
					<xsd:element name="Application" type="BagAppSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Baggage dimensions application. Examples: Party, Traveler

Note: Party applies to all Travelers.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Origin" type="AirportCodeType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Origin Airport or City code. Example: TPA
Encoding Scheme: IATA/ A4A Aiport or City Code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Destination" type="AirportCodeType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Destination Airport or City code. Example: ORD
Encoding Scheme: IATA/ A4A Aiport or City Code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Allowances" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Baggage Allowance representation, including allowance text description, allowance by piece, allowance by bag dimensions and allowance by weight.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element ref="WeightAllowance"/>
								<xsd:element ref="PieceAllowance"/>
								<xsd:element ref="DimensionAllowance"/>
								<xsd:element ref="AllowanceDescription"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="MS_Carrier" type="AirlineID_Type" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Most significant carrier.
Examples: BA, British Airways

Note: This is the first carrier crossing the IATA Tariff Area (for travel across one Tariff Area) per IATA Resolution 302 - Baggage Provisions Selection Criteria.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Details" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Baggage Pricing and Details.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Details" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Baggage details.</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="Restrictions" minOccurs="0">
												<xsd:annotation>
													<xsd:documentation source="description" xml:lang="en">Baggage restriction information per Traveler and/ or per party.</xsd:documentation>
												</xsd:annotation>
												<xsd:complexType>
													<xsd:sequence>
														<xsd:element name="Restriction" maxOccurs="unbounded">
															<xsd:annotation>
																<xsd:documentation source="description" xml:lang="en">Baggage restriction detail.</xsd:documentation>
															</xsd:annotation>
															<xsd:complexType>
																<xsd:complexContent>
																	<xsd:extension base="AssociatedObjectBaseType">
																		<xsd:choice>
																			<xsd:element name="CheckedBag">
																				<xsd:annotation>
																					<xsd:documentation source="description" xml:lang="en">Checked Baggage Allowance.</xsd:documentation>
																				</xsd:annotation>
																				<xsd:complexType>
																					<xsd:sequence>
																						<xsd:element ref="WeightAllowance"/>
																						<xsd:element ref="PieceAllowance"/>
																						<xsd:element ref="DimensionAllowance"/>
																						<xsd:element ref="AllowanceDescription"/>
																					</xsd:sequence>
																				</xsd:complexType>
																			</xsd:element>
																			<xsd:element name="CarryOn">
																				<xsd:annotation>
																					<xsd:documentation source="description" xml:lang="en">Carry-on Allowance.</xsd:documentation>
																				</xsd:annotation>
																				<xsd:complexType>
																					<xsd:sequence>
																						<xsd:element ref="BaggageAllowanceGroup">
																							<xsd:annotation>
																								<xsd:documentation source="description" xml:lang="en">Baggage Allowance representation choices.

A choice between specifying:
- Maximum piece(s)
- Maximum weight
- Maximum dimensions
- Carrier published description</xsd:documentation>
																							</xsd:annotation>
																						</xsd:element>
																					</xsd:sequence>
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:choice>
																	</xsd:extension>
																</xsd:complexContent>
															</xsd:complexType>
														</xsd:element>
													</xsd:sequence>
												</xsd:complexType>
											</xsd:element>
											<xsd:element name="AvailableDiscounts" minOccurs="0">
												<xsd:annotation>
													<xsd:documentation source="description" xml:lang="en">Available baggage discounts.

Note: If specified, FQTV applies at a program (not individual Traveler) level.</xsd:documentation>
												</xsd:annotation>
												<xsd:complexType>
													<xsd:sequence>
														<xsd:element name="AvailableDiscount" type="DiscountType" maxOccurs="unbounded">
															<xsd:annotation>
																<xsd:documentation source="description" xml:lang="en">Available baggage discount.</xsd:documentation>
															</xsd:annotation>
														</xsd:element>
													</xsd:sequence>
												</xsd:complexType>
											</xsd:element>
											<xsd:element name="Prices" minOccurs="0">
												<xsd:annotation>
													<xsd:documentation source="description" xml:lang="en">Baggage allowance pricing.</xsd:documentation>
												</xsd:annotation>
												<xsd:complexType>
													<xsd:sequence>
														<xsd:element name="Price" maxOccurs="unbounded">
															<xsd:annotation>
																<xsd:documentation source="description" xml:lang="en">Baggage allowance pricing detail.</xsd:documentation>
															</xsd:annotation>
															<xsd:complexType>
																<xsd:choice>
																	<xsd:element ref="AwardPricing"/>
																	<xsd:element ref="CombinationPricing"/>
																	<xsd:element ref="DetailCurrencyPrice"/>
																	<xsd:element ref="EncodedCurrencyPrice"/>
																	<xsd:element ref="SimpleCurrencyPrice"/>
																</xsd:choice>
															</xsd:complexType>
														</xsd:element>
													</xsd:sequence>
												</xsd:complexType>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element ref="Remarks" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Carrier published description information.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="BagAllowanceDimensionType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Baggage Allowance by Dimensions.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AssociatedObjectBaseType">
				<xsd:sequence>
					<xsd:element name="ApplicableParty" type="BagAppSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Baggage weight restriction application. Examples: Party, Traveler

Note: Party applies to all Travelers.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DimensionUOM" type="SizeUnitSimpleType">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Baggage dimension UOM (unit of measure). Example: Centimeter, Inch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:choice>
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">A choice between specifying baggage dimensions as:
- Total linear dimensions
- Individual length, height and/ or width</xsd:documentation>
						</xsd:annotation>
						<xsd:sequence>
							<xsd:element name="MaxLinear">
								<xsd:annotation>
									<xsd:documentation source="description" xml:lang="en">Maximum linear dimensions value. Example: 62

Notes:
1. Linear measurement = length + width + height
2. Size unit of measure is specified in BagUOM.</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:simpleContent>
										<xsd:extension base="xsd:string">
											<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
										</xsd:extension>
									</xsd:simpleContent>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="MinLinear">
								<xsd:annotation>
									<xsd:documentation>Minimum linear dimensions value. Example: 20

Notes:
1. Linear measurement = length + width + height
2. Size unit of measure is specified in BagUOM.</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:simpleContent>
										<xsd:extension base="xsd:string">
											<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
										</xsd:extension>
									</xsd:simpleContent>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
						<xsd:element name="Dimensions" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Maximum baggage dimension category and value.

Notes: Baggage Size UOM (unit of measure) is specified in BagUOM.</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="Category" type="BagDimensionSimpleType">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="en">Baggage dimensions category.
Examples: Length, Height, Width</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="MaxValue" type="xsd:decimal">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="en">Maximum dimension value.  Example: 30</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="MinValue" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Minimum dimension value.  Example: 10</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
								<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
							</xsd:complexType>
						</xsd:element>
					</xsd:choice>
					<xsd:element name="ApplicableBag" type="ProperNameSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Applicable bag type.
Examples: Carry-On, 1st Checked Bag, 2nd Checked Bag</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Descriptions" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="BagAllowancePieceType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Baggage Allowance by Piece.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AssociatedObjectBaseType">
				<xsd:sequence>
					<xsd:element name="ApplicableParty" type="BagAppSimpleType">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Baggage weight restriction application. Examples: Party, Traveler

Note: Party applies to all Travelers.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="TotalQuantity" type="xsd:positiveInteger">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Maximum pieces.
Example: 4</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="BagType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Type of Bag,.
Example: Golf Equipment, Bag, Fishing, Car Seat</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ApplicableBag" type="ProperNameSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Applicable bag type.
Examples: Carry-On, 1st Checked Bag, 2nd Checked Bag</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Descriptions" minOccurs="0"/>
					<xsd:element name="PieceMeasurements" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Provides information on per piece weight and dimensions.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="PieceWeightAllowance" type="BagAllowanceWeightType" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>BAGGAGE WEIGHT allowance per baggage piece.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="PieceDimensionAllowance" type="BagAllowanceDimensionType" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>BAGGAGE DIMENSION allowance per baggage piece.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
							<xsd:attribute name="Quantity" type="xsd:int" use="required">
								<xsd:annotation>
									<xsd:documentation>Quantity of pieces with these measurements.</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="PieceAllowanceCombination">
					<xsd:annotation>
						<xsd:documentation>Provides logical combiinaton between  allowances pieces. Enumaration with OR and AND as posisble values.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="OR"/>
							<xsd:enumeration value="AND"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="BagAllowanceWeightType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Baggage Allowance by Weight.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AssociatedObjectBaseType">
				<xsd:sequence>
					<xsd:element name="ApplicableParty" type="BagAppSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Baggage weight restriction application. Examples: Party, Traveler

Note: Party applies to all Travelers.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="MaximumWeight" maxOccurs="2">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Maximum weight value and unit of measure.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Value" type="xsd:decimal">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Maximum weight value.  Example: 50</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="UOM" type="WeightUnitSimpleType">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Baggage weight unit of measure.  Example: Kilogram, Pound</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="SpreadOverBagsQuantity" type="xsd:int" minOccurs="0"/>
							</xsd:sequence>
							<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="ApplicableBag" type="ProperNameSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Applicable bag type.
Examples: Carry-On, 1st Checked Bag, 2nd Checked Bag</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Descriptions" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="BagAllowanceDescType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Baggage Allowance by Description.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AssociatedObjectBaseType">
				<xsd:sequence>
					<xsd:element name="ApplicableParty" type="BagAppSimpleType">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Baggage weight restriction application.  Examples: Party, Traveler

Note: Party applies to all Travelers.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ApplicableBag" type="ProperNameSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Applicable bag type.
Examples: Carry-On, 1st Checked Bag, 2nd Checked Bag</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Descriptions"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="BagCarryOnBaseType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Carry-on Baggage Allowance.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="KeyWithMetaObjectBaseType">
				<xsd:sequence>
					<xsd:element name="Origin" type="AirportCodeType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Origin Airport or City code. Example: TPA
Encoding Scheme: IATA/ A4A Aiport or City Code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Destination" type="AirportCodeType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Destination Airport or City code. Example: ORD
Encoding Scheme: IATA/ A4A Aiport or City Code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="MS_Carrier" type="AirlineID_Type" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Most significant carrier.
Examples: BA, British Airways

Note: This is the first carrier crossing the IATA Tariff Area (for travel across one Tariff Area) per IATA Resolution 302 - Baggage Provisions Selection Criteria.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Descriptions" minOccurs="0"/>
					<xsd:element name="Details" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Carry-on Baggage details.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Prices" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Carry-on baggage allowance pricing.</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="Price" maxOccurs="unbounded">
												<xsd:annotation>
													<xsd:documentation source="description" xml:lang="en">Baggage allowance pricing detail.</xsd:documentation>
												</xsd:annotation>
												<xsd:complexType>
													<xsd:complexContent>
														<xsd:extension base="PriceCoreType">
															<xsd:sequence>
																<xsd:element name="ApplicableBag" type="xsd:string">
																	<xsd:annotation>
																		<xsd:documentation source="description" xml:lang="en">Applicable carry-on bag type.
Examples: First</xsd:documentation>
																	</xsd:annotation>
																</xsd:element>
															</xsd:sequence>
														</xsd:extension>
													</xsd:complexContent>
												</xsd:complexType>
											</xsd:element>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="BagDisclosureRuleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Baggage Disclosure Rule.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="BagDiscloseRuleSimpleType">
				<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="BagDisclosureType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for US Department of Transportation (DOT) Reservation or Disclosure baggage rule.</xsd:documentation>
			<xsd:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value.
=enumerated values=
N = NOT SUBJECT TO US DOT RESERVATION OR DISCLOSURE
Y = SUBJECT TO US DOT RESERVATION AND DISCLOSURE
D = SUBJECT TO DISCLOSURE BUT NOT TO US DOT RESERVATION</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="KeyWithMetaObjectBaseType">
				<xsd:sequence>
					<xsd:element name="BagRule" type="BagDiscloseRuleSimpleType">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">US Department of Transportation (DOT) Reservation or Disclosure baggage rule.
Examples: Y, Disclosure AND US DOT Reservation</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Descriptions" minOccurs="0"/>
					<xsd:element ref="Remarks" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="BaggageType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Baggage definition.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Associations" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Baggage Association(s), including Travelers; Flight O/D and segment(s).</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Traveler">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Anonymous, Known and/or PTC that the offer applies to.

Note: Traveler references  defined here apply to this Offer Item and will override   Offer level information.</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:choice>
									<xsd:element ref="TravelerReference">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="en">Travelers reference Key.

Both anonymous and known Travelers may be specified.</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="PTC">
										<xsd:annotation>
											<xsd:documentation source="description" xml:lang="en">Passenger type code applicable to all offers. Example: ADT

Encoding Scheme:  IATA (three character) Passenger Type Code</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:simpleContent>
												<xsd:extension base="PaxSimpleType"/>
											</xsd:simpleContent>
										</xsd:complexType>
									</xsd:element>
								</xsd:choice>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Offer" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Other Offer and Offer Item Association(s).</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:attribute name="refs_oitem" type="InstanceClassRefSimpleType" use="optional">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Reference to Offer Item instance(s). Example: oitem1</xsd:documentation>
									</xsd:annotation>
								</xsd:attribute>
								<xsd:attribute name="refs_offer" type="InstanceClassRefSimpleType" use="optional">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Reference to Offer instance(s). Example: offer1</xsd:documentation>
									</xsd:annotation>
								</xsd:attribute>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Flight" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Origin and Destination/ Flight Association(s).</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:attribute name="refs_od" type="InstanceClassRefSimpleType" use="optional">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Reference to Origin/Destination instance(s). Example: od1</xsd:documentation>
									</xsd:annotation>
								</xsd:attribute>
								<xsd:attribute name="refs_flight" type="InstanceClassRefSimpleType" use="optional">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Reference to Flight Segment instance(s). Example: segment1</xsd:documentation>
									</xsd:annotation>
								</xsd:attribute>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="OptionalService" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation source="description" xml:lang="en">Optional Service and Service Bundle Association(s).</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:attribute name="refs_service" type="InstanceClassRefSimpleType" use="optional">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Reference to Optional Service instance(s). Example: service1</xsd:documentation>
									</xsd:annotation>
								</xsd:attribute>
								<xsd:attribute name="refs_bundle" type="InstanceClassRefSimpleType" use="optional">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Reference to Optional Service Bundle instance(s). Example: sbundle1</xsd:documentation>
									</xsd:annotation>
								</xsd:attribute>
							</xsd:complexType>
						</xsd:element>
						<xsd:element ref="DepartureCode" minOccurs="0"/>
						<xsd:element ref="ArrivalCode" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Disclosure" type="BagDisclosureType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">Baggage Disclosure information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CheckedBags" minOccurs="0"/>
			<xsd:element ref="CarryOnBags" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BaggageChargeType">
