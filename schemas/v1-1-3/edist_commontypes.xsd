					<xsd:annotation>
						<xsd:documentation source="description" xml:lang="en">Location-related Metadata, including Directions.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="Offer" minOccurs="0"/>
				<xsd:element ref="Pricing" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation source="description" xml:lang="en">Pricing-related Metadata, including Discount.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="Qualifiers" minOccurs="0"/>
