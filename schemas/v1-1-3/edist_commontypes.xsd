							<xsd:documentation source="description" xml:lang="en">Pseudo City code. Example: AAA2</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:extension base="PseudoCitySimpleType">
