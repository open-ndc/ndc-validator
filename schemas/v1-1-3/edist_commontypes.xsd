	<xsd:element name="ShopMetadataGroup" abstract="true">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Shopping Metadata choices.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Baggage" minOccurs="0"/>
				<xsd:element ref="Fare" minOccurs="0"/>
