		<xsd:complexContent>
			<xsd:extension base="MetadataObjectBaseType">
				<xsd:sequence>
					<xsd:element name="TailNumber" type="ContextSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Aircraft tail number.  Example: N387SW</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Name" type="ProperNameSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Aircraft name.Example: N387SW</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
