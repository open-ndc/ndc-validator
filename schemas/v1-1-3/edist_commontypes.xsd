						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Meal(s) Served.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Meal" type="FlightMealsType" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Meal served code. Example: B (Breakfast)

Encoding Scheme: IATA Padis Codeset - List for data element 7161 - Special Service, Coded</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
