		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MetadataObjectBaseType">
				<xsd:sequence>
					<xsd:element name="HistoricPricingDate" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Historic pricing information.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="Date" type="xsd:date">
									<xsd:annotation>
