		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AircraftEquipment"/>
			<xsd:enumeration value="BaggageDisclosure"/>
			<xsd:enumeration value="CarryOnAllowance"/>
			<xsd:enumeration value="CheckedBagAllowance"/>
			<xsd:enumeration value="MediaGroup"/>
			<xsd:enumeration value="MediaItem"/>
			<xsd:enumeration value="OfferDisclosure"/>
			<xsd:enumeration value="OfferInstructions"/>
