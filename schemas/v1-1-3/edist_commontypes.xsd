										<xsd:annotation>
											<xsd:documentation>Defines where the PCC was created. For example 1A, 1P, 1S</xsd:documentation>
										</xsd:annotation>
									</xsd:attribute>
								</xsd:extension>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="IATA_Number" type="IATA_NbrSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Agency Seller IATA number. Example: 98417900</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
