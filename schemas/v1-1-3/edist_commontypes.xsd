	<xsd:complexType name="FareMetadataType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">FARE Metadata definition.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MetadataObjectBaseType">
				<xsd:sequence>
					<xsd:element ref="FareCacheKey" minOccurs="0"/>
					<xsd:element name="FareReferenceKey" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Unique fare reference key. Example: PQ123456</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:simpleContent>
								<xsd:extension base="ShortDescSimpleType">
									<xsd:attributeGroup ref="ObjAssociationAttrGroup"/>
								</xsd:extension>
							</xsd:simpleContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="AutoExchange" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Auto-Exchange Solution information.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="ExchangeSolutionType" type="CodesetType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Auto-exchange solution type. Example: R (REISSUE)

Encoding Scheme: IATA PADIS Codeset AES (Auto Exchange Solution)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ExchangeCalcType" type="FareExchangeSimpleType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Fare exchange calculation method.
Examples:
BASE (BASE FARE USED)
EQV (EQUIVALENT FARE USED)</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ChangeType" type="FareExchangeSimpleType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation source="description" xml:lang="en">Applicable change type.
Examples: Refund, AdditionalCharges, Even</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="FareComponentMetadataType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">FARE COMPONENT Metadata definition.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MetadataObjectBaseType">
				<xsd:sequence>
					<xsd:element name="PriceQuoteDate" type="xsd:date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The date on which the fare was calculated and quoted. Example: 2017-01-13</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PricingDesignator" type="ContextSimpleType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>A qualifier for pricing to select fares. Example: P2Y</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="FareDetailMetadataType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">FARE DETAIL Metadata definition.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MetadataObjectBaseType">
				<xsd:sequence>
					<xsd:element name="FareTextInd" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Fare text indicator.
Examples:
A (FULL TEXT)
B (MINI RULE)</xsd:documentation>
							<xsd:documentation source="ENCODING" xml:lang="en">Select an enumerated value from the list or enter a string value.
=enumerated values=
A = FULL TEXT
B = MINI RULE</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="FullText"/>
								<xsd:enumeration value="MiniRule"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="HistoricPricingDate" type="xsd:date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Historic date used to price this fare group. Example: 2017-12-25</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="TicketedDateTime" type="xsd:dateTime" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Historical ticketed date/ time information.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PricingEngine" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="description" xml:lang="en">Information Returned from a Pricing Engine, including Market Name and Mileage Zone.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element ref="PricedMarketName" minOccurs="0"/>
								<xsd:element ref="PricedMileageZone" minOccurs="0"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
