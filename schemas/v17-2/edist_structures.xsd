		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Coupon Media Type contextual list.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Electronic"/>
			<xsd:enumeration value="Paper"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CouponMediaSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Baggage Application.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType CouponMediaListType"/>
	</xsd:simpleType>
	<xsd:simpleType name="CouponPurposeListType">
