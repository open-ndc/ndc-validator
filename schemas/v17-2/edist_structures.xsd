			<xsd:enumeration value="Net">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Refund"/>
			<xsd:enumeration value="Sell">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
