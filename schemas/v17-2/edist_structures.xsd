	</xsd:simpleType>
	<xsd:simpleType name="ConnectMethodSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Currency Code Application.

Examples: Actual, Display, Requested, Conversion, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType ConnectMethodListType"/>
	</xsd:simpleType>
	<!--<< CONTACT >>-->
	<xsd:simpleType name="ContactListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Currency Code Application constrained list.

Examples: Actual, Display, Requested, Conversion, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Billing"/>
			<xsd:enumeration value="Business"/>
			<xsd:enumeration value="Home"/>
