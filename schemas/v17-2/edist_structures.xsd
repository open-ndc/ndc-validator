			<xsd:documentation source="description" xml:lang="en">A data type for Fare Rule Modification or Override Type contextual list.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ExchangedReissued"/>
			<xsd:enumeration value="FareComponent"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FareWaiverRuleSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Baggage Application.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType FareWaiverRuleListType"/>
	</xsd:simpleType>
	<!--<< CURRENCY >>-->
	<xsd:attributeGroup name="AmountCurrencyGroup">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Currency encoding for the Amount value.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="Code" type="CurrencySimpleType" use="required">
			<xsd:annotation>
				<xsd:documentation source="description" xml:lang="en">Currency code. Example: USD
Encoding Scheme: ISO 4217 (three alpha character) Currency Code</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<xsd:attributeGroup name="AmountCurrencyOptGroup">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">Optional currency encoding for the Amount value.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="Code" type="CurrencySimpleType" use="optional">
			<xsd:annotation>
				<xsd:documentation source="description" xml:lang="en">Currency code. Example: USD
Encoding Scheme: ISO 4217 (three alpha character) Currency Code</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<xsd:simpleType name="CurrencyListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Currency Code Application list constraint.

Examples: Actual, Display, Requested, Conversion, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Actual"/>
			<xsd:enumeration value="Conversion"/>
			<xsd:enumeration value="Display"/>
			<xsd:enumeration value="Requested"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CurrencyAppSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Currency Code Application.

Examples: Actual, Display, Requested, Conversion, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType CurrencyListType"/>
	</xsd:simpleType>
	<xsd:simpleType name="CurrencySimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Currency Code encoding constraint: ISO 4217 (three alpha character) Currency Code</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< DATE/ TIME FORMATS >>-->
	<xsd:simpleType name="DayOfWeekMaskType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Day of Week encoding constraint:String, 1 - 7 length</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="7"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DayOfWeekSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Day of Week encoding constraint:Positive Integer 1 - 7</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0]{1}[1-7]{1}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MonthNumberSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Month Number encoding constraint: 01 through 12</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-1]{1}[0-9]{1}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TimeSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Time encoding constraint: SO 8601 Military/ 24 Hour (HH:MM)
Example: 14:00</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="5"/>
			<xsd:pattern value="([0-1][0-9]|2[0-3]):[0-5][0-9]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< DESCRIPTIONS >>-->
	<xsd:simpleType name="DescriptionSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Description encoding constraint: Minimum Length of 1</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ShortDescSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Short Description size constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< DOMAIN >>-->
	<xsd:simpleType name="DomainSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for NDC Domain List.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AirlineProfile"/>
			<xsd:enumeration value="OrderManagement"/>
			<xsd:enumeration value="PaymentTicketing"/>
			<xsd:enumeration value="Shopping"/>
			<xsd:enumeration value="Utility"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< DISTANCE >>-->
	<xsd:simpleType name="DistanceUnitListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Distance Unit of Measure contextual list.
Examples: Miles, Kilometers</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Feet"/>
			<xsd:enumeration value="Kilometers">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">K = KILOMETERS</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Meters"/>
			<xsd:enumeration value="Miles">
				<xsd:annotation>
					<xsd:documentation source="description" xml:lang="en">M = MILES</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DistanceUnitSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Currency Code Application.

Examples: Actual, Display, Requested, Conversion, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType DistanceUnitListType"/>
	</xsd:simpleType>
	<!--<< IMAGE >>-->
	<xsd:simpleType name="ImageUnitListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for an Image UOM constrained list.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Inch"/>
			<xsd:enumeration value="Millimeter"/>
			<xsd:enumeration value="Pixel"/>
			<xsd:enumeration value="Point"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ImageUnitSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for an Image UOM Description.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ContextSimpleType ImageUnitListType"/>
	</xsd:simpleType>
	<!--<< LANGUAGE >>-->
	<xsd:simpleType name="LanguageAppListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application list constraint.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Actual"/>
			<xsd:enumeration value="Requested"/>
			<xsd:enumeration value="Spoken"/>
			<xsd:enumeration value="Written"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LanguageAppSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType LanguageAppListType"/>
	</xsd:simpleType>
	<xsd:simpleType name="RenderingTypeListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Rendering Type constrained list.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Narrative"/>
			<xsd:enumeration value="XSLT"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RenderingTypeSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType RenderingTypeListType"/>
	</xsd:simpleType>
	<!--<< PASSENGER TYPE >>-->
	<xsd:simpleType name="PaxSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Passenger Type Code (PTC) encoding constraint: IATA (three character) Passenger Type Codes</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="3"/>
			<xsd:pattern value="[0-9A-Z]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< PAYMENT >>-->
	<xsd:simpleType name="PayCardTypeSimpleType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">A data type for a Credit Card Type constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:pattern value="[0-9a-zA-Z]{1,6}"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>
	<xsd:simpleType name="PayCardCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">A data type for a Credit Card Code constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:pattern value="[A-Z]{2}"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>
	<xsd:simpleType name="PayCardNmbrSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Credit Card Number constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{1,19}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PayCardCVV_SimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Payment Card Series Code constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{1,8}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PayCardDateSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Payment Card Date constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(0[1-9]|1[0-2])[0-9][0-9]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentStatusListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for Payment Status contextual list. Examples: Canceled, Complete, Failed, MoreInformation, Partial
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Canceled"/>
			<xsd:enumeration value="Complete"/>
			<xsd:enumeration value="Failed"/>
			<xsd:enumeration value="MoreInformation"/>
			<xsd:enumeration value="Partial"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MaskedCardNmbrSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Masked Payment Card Number constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9a-zA-Z]{1,19}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< PENALTY >>-->
	<xsd:simpleType name="PenaltyAppListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Offer Penalty Amount Application list constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="MaximumPenaltyAmount"/>
			<xsd:enumeration value="MinimumPenaltyAmount"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PenaltyAppSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Offer Penalty Amount Application.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType PenaltyAppListType"/>
	</xsd:simpleType>
	<xsd:simpleType name="PenaltyListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Offer Penalty Type list constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Cancellation"/>
			<xsd:enumeration value="Change"/>
			<xsd:enumeration value="NoShow"/>
			<xsd:enumeration value="Upgrade"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PenaltySimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Offer Penalty Type.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType PenaltyListType"/>
	</xsd:simpleType>
	<!--<< PERCENTAGE >>-->
	<xsd:simpleType name="WholePercentageSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for  Whole Percentage encoding constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:minInclusive value="0.01"/>
			<xsd:maxInclusive value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< PREFERENCE >>-->
	<xsd:simpleType name="PreferencesLevelListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Preference Type list constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Preferred"/>
			<xsd:enumeration value="Required"/>
			<xsd:enumeration value="Exclude"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PreferencesLevelSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType PreferencesLevelListType"/>
	</xsd:simpleType>
	<!--<< PROPER NAME >>-->
	<xsd:simpleType name="ProperNameSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Proper Name size constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CarrierNameType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Carrier Name size constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="99"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyNameSimpleType_">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Property Name size constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< PROXIMITY >>-->
	<xsd:simpleType name="ProximityAppListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Proximity Data Application list constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ActualProximity"/>
			<xsd:enumeration value="SearchRadius"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ProximityAppSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Proximity Data Application.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType ProximityAppListType"/>
	</xsd:simpleType>
	<!--<< PSEUDO CITY >>-->
	<xsd:simpleType name="PseudoCitySimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for  Pseudo City Code encoding constraint. Example: AAA2</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="16"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< REDEMPTION PRICING >>-->
	<xsd:simpleType name="RedempUnitListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Redemption Unit List.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Miles"/>
			<xsd:enumeration value="Points"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RedemptionUnitSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType RedempUnitListType"/>
	</xsd:simpleType>
	<!--<< RULES >>-->
	<xsd:simpleType name="RuleStatusListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Rule Status contextual list.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ActionRequired"/>
			<xsd:enumeration value="RuleExecuted"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RuleStatusSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType RuleStatusListType"/>
	</xsd:simpleType>
	<!--<< SPECIAL SERVICE >>-->
	<xsd:simpleType name="SpecialSrvcSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Special Service or Request Code constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="SSR_SimpleType CodesetValueSimpleType"/>
	</xsd:simpleType>
	<xsd:simpleType name="SSR_SimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Special Service Request Code encoding constraint: IATA
Examples:
EXST (EXTRA SEAT REQUIRED)
INFT (INFANT ACCOMPANYING ADULT PASSENGER)
PETC (PASSENGER TRAVELING WITH PET IN CABIN)
UMNR (UNACCOMPANIED MINOR)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z]{4}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< STANDARDS >>-->
	<xsd:simpleType name="IATA_NbrSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for IATA Company Number encoding constraint. Example: 98417900</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z0-9]{7,8}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ICAO_LocSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for ICAO Location Code encoding constraint. Example: K (CONTIGUOUS UNITED STATES)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< TICKET >>-->
	<xsd:simpleType name="TicketDesigAppListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Ticket Designator Application list constraint.

Examples: Requested, Ticketed, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Requested"/>
			<xsd:enumeration value="Ticketed"/>
			<xsd:enumeration value="Other"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TicketDesigAppSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Language Code Application.

Examples: Display, Requested, Spoken, Written, Other</xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType TicketDesigAppListType"/>
	</xsd:simpleType>
	<xsd:simpleType name="TicketNumberSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for Ticket Number Constraint. Examples: String, Length 1 to 16.
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="16"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< TRIP >>-->
	<xsd:simpleType name="TripPurposeListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Trip Purpose contextual list.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Business"/>
			<xsd:enumeration value="Leisure"/>
			<xsd:enumeration value="BusinessAndLeisure"/>
			<xsd:enumeration value="Other"/>
			<xsd:enumeration value="Unknown"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< UNIQUE IDENTIFIER >>-->
	<xsd:simpleType name="UniqueStringID_SimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for a Unique String Identifier constraint.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--<< UNITS OF MEASURE >>-->
	<xsd:simpleType name="SizeUnitSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">A data type for Size Values list constraint. Examples: Centimeter, Inch</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Centimeter"/>
			<xsd:enumeration value="Inch"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- << POLICY >> -->
	<xsd:simpleType name="PolicyTypeSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for Policy Type list constraint. Examples: Privacy_PII, Payment_PCI, Credentialing
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Privacy_PII">
				<xsd:annotation>
					<xsd:documentation>
            Personnally Identifting Information Privacy Policy.
          </xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Payment_PCI"/>
			<xsd:enumeration value="Credentialing"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PolicyTypeListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for Policy Type. Examples: Privacy_PII, Payment_PCI, Credentialing
      </xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType PolicyTypeSimpleType"/>
	</xsd:simpleType>
	<!-- << RESHOPPING >> -->
	<xsd:simpleType name="AlphaNumericStringLength1to3">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Alpha-Numeric Strings, length 1 to 3</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9a-zA-Z]{1,3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AlphaLength3">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Alphabetic Strings, length exactly 3</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-zA-Z]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AirlineType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Type definition for IATA airline designator (2-alphanumeric) or ICAO airline code (3-alpha).</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([0-9A-Z]{2})|([A-Z]{3})"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AlphaNumericStringLength1to71">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Alpha-Numeric Strings, length 1 to 71</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9a-zA-Z]{1,71}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AlphaNumericStringLength1to4">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used forAlpha-Numeric Strings, length 1 to 4.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9a-zA-Z]{1,4}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Numeric0to99">
		<xsd:annotation>
			<xsd:documentation>Use for numeric values, from 0 to 99 inclusive.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="99"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NumericStringLength1to19">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Numeric Strings, length 1 to 19.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{1,19}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OrderItemActionSimpleType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for an Order Action list constraint. Examples: Cancel, Create, Divide, Delete, Reissue, Reprice, Update
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Cancel"/>
			<xsd:enumeration value="Create"/>
			<xsd:enumeration value="Delete"/>
			<xsd:enumeration value="Divide"/>
			<xsd:enumeration value="Reissue"/>
			<xsd:enumeration value="Reprice"/>
			<xsd:enumeration value="Update"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MeasurementTypeListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
       Examples: Imperial, Metric
      </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Imperial"/>
			<xsd:enumeration value="Metric"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OrderItemActionListType">
		<xsd:annotation>
			<xsd:documentation source="description" xml:lang="en">
        A data type for an Order Action. Examples: Create, Update, Delete, Reissue, Reprice
      </xsd:documentation>
		</xsd:annotation>
		<xsd:union memberTypes="ApplicationSimpleType OrderItemActionSimpleType"/>
	</xsd:simpleType>
	<xsd:simpleType name="BaggageCategoryListType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CarryOn"/>
			<xsd:enumeration value="Checked"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BeforeOrAfterListType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Before"/>
			<xsd:enumeration value="After"/>
